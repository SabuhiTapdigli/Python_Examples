    "import math\n",
    "def polysum(n,s):\n",
    "    area=(0.25*n*(s**2))/(math.tan((math.pi)/n))\n",
    "    perm=n*s\n",
    "    sum=area+(perm**2)\n",
    "    return round(sum,4)\n",
    "polysum(5,4)"
   ]
  }
